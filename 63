package com.Assignments;

import java.util.ArrayList;

public class No_63 {
    public static void main(String[] args) {
        ArrayList<String> a1 = new ArrayList<>();

     
        a1.add("Apple");
        a1.add("Banana");
        a1.add("Cherry");
        a1.add("Dragonfruit");
        a1.add("Grapes");

        
        ArrayList<String> a2 = new ArrayList<>(a1.subList(1, 4));

        System.out.println(a2);
    }
}
