package com.Assignments;

import java.util.Scanner;

public class No_1 {
	public static void main(String[] args) {
		Scanner scan  = new Scanner(System.in);
		
		System.out.println("Enter Three numbers");
		int number1 = scan.nextInt();
		int number2 = scan.nextInt();
		int number3 = scan.nextInt();
		if(number1>number2 && number1>number3) {
		System.out.println("The greatest number out of three numbers is  " +number1);
		}
		if(number2>number1&& number2 >number3) {
			System.out.println("The greatest number out of three numbers is  " +number2);
			}
		if(number3>number1&& number3>number2) {
			System.out.println("The greatest number out of three numbers is  " +number3);
			}
	}

}
